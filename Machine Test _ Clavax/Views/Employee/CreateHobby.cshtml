@model Machine_Test___Clavax.DB.Hobby_Master
@{
    ViewBag.Title = "Create Hobby";
}

<h2>Create Hobby</h2>

@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <h4>Hobby Master</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.hobbyName, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.hobbyName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.hobbyName, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "HobbyList")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}